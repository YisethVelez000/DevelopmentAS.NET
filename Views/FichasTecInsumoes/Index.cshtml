@model IEnumerable<DevelopmentAS.Models.FichasTecInsumo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Cantidad)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdFichaNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdInsumoNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Cantidad)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdFichaNavigation.IdFicha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdInsumoNavigation.IdInsumo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdFichaTecIns">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdFichaTecIns">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdFichaTecIns">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
